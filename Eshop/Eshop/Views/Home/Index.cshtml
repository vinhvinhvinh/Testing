<section class="welcome_bakery_area pink_cake_feature">
    <div class="container">
        <div class="cake_feature_inner">
            <div class="title_view_all">
                <div class="float-left">
                    <div class="main_w_title">
                        <h2>Cake Categories</h2>
                        <h5> Seldolor sit amet consect etur</h5>
                    </div>
                </div>
                <div class="float-right">
                    <a asp-controller="Home" asp-action="Cake" class="pest_btn">View all Products</a>
                </div>
            </div>
            <div class="cake_feature_slider owl-carousel">

                @foreach (ProductType t in ViewBag.AllType)
                {
                    <div class="item">
                        <div class="cake_feature_item">
                            <a asp-controller="Home" asp-action="ProductByType" asp-route-id="@t.Id">
                                <div class="cake_img">
                                    <img src="~/img/service/@t.Image" alt="">
                                </div>
                            </a>
                            <div class="cake_text">
                                <h3>@t.Name</h3>
                                <a class="pest_btn"asp-controller="Home"  asp-action="ProductByType" asp-route-id="@t.Id">View more</a>
                            </div>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
</section>
